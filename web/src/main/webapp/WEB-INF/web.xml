<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
    <servlet-name>serv</servlet-name>
    <servlet-class>org.example.servlet.ServletTest</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>serv</servlet-name>
    <url-pattern>/serv</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>sav</servlet-name>
    <servlet-class>org.example.servlet.SavingsServletTest</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>sav</servlet-name>
    <url-pattern>/sav</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>bird</servlet-name>
    <servlet-class>org.example.servlet.WeaverTest</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>bird</servlet-name>
    <url-pattern>/bird</url-pattern>
  </servlet-mapping>
  
  
  
</web-app>
